{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"textdomain": "snow-monkey-search",
	"name": "snow-monkey-search/item",
	"title": "Item",
	"category": "snow-monkey-search",
	"parent": [ "snow-monkey-search/search-box" ],
	"allowedBlocks": [
		"snow-monkey-search/custom-field-search",
		"snow-monkey-search/keyword-search",
		"snow-monkey-search/period-search",
		"snow-monkey-search/taxonomy-search"
	],
	"attributes": {
		"flexGrow": {
			"type": "boolean",
			"default": false
		},
		"flexShrink": {
			"type": "boolean",
			"default": true
		},
		"flexBasis": {
			"type": "string",
			"default": ""
		},
		"templateLock": {
			"type": [ "string", "boolean" ],
			"enum": [ "all", "insert", "contentOnly", false ]
		}
	},
	"supports": {
		"className": false,
		"anchor": true,
		"html": false
	},
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css"
}
