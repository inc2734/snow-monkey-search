{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"textdomain": "snow-monkey-search",
	"name": "snow-monkey-search/custom-field-search",
	"title": "Custom Field Search",
	"category": "snow-monkey-search",
	"parent": [ "snow-monkey-search/item" ],
	"attributes": {
		"label": {
			"type": "string",
			"default": null
		},
		"key": {
			"type": "string",
			"default": ""
		},
		"postType": {
			"type": "string",
			"default": ""
		},
		"controlType": {
			"type": "string",
			"default": "text"
		},
		"options": {
			"type": "string",
			"default": ""
		},
		"compare": {
			"type": "string",
			"default": "="
		},
		"type": {
			"type": "string",
			"default": "char"
		},
		"flow": {
			"type": "string",
			"default": "inline"
		},
		"itemMinWidth": {
			"type": "string",
			"default": ""
		}
	},
	"usesContext": [ "snow-monkey-search/relatedPostType" ],
	"supports": {
		"className": false,
		"anchor": true,
		"html": false
	},
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
